{
  "expo": {
    "name": "spaceship-titanic",
    "slug": "spaceship-titanic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spaceship_titanic_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spaceship_titanic_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5a4aaf"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/spaceship_titanic_adaptive_icon.png",
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "favicon": "./assets/spaceship_titanic_adaptive_icon.png"
    }
  }
}